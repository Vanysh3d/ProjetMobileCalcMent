<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

    <application
        android:allowBackup="true"
        android:dataExtractionRules="@xml/data_extraction_rules"
        android:fullBackupContent="@xml/backup_rules"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.ProjetMobileCalcMent"
        tools:targetApi="31">
        <service
            android:name="com.example.ProjectMobileCalcMent.MusicService"
            android:enabled="true"
            android:exported="true"></service>

        <activity
            android:name="com.example.ProjectMobileCalcMent.SettingsActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ProjectMobileCalcMent.RegistrationActivity"
            android:exported="false"
            android:theme="@style/Theme.ProjetMobileCalcMent" />
        <activity
            android:name="com.example.ProjectMobileCalcMent.ScoreActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ProjectMobileCalcMent.GameActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ProjectMobileCalcMent.AboutActivity"
            android:exported="false" />
        <activity
            android:name="com.example.ProjectMobileCalcMent.HomeScreen"
            android:exported="true"
            android:theme="@style/Theme.ProjetMobileCalcMent">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>